<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="">
	<select id="select"  resultType="com.gaon.cinema.login.LoginDTO" parameterType="com.gaon.cinema.login.LoginDTO">
		select * from GC_MEMBER
	</select>
	
	<select id="count"  resultType="int" parameterType="com.gaon.cinema.login.LoginDTO">
		select count(*) cnt from GC_member where USERID=#{id} and USERPW=#{pw}
	</select>
</mapper>















