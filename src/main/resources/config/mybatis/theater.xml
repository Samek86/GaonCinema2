<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="theater">
	
	<!-- 도시 이름 가져오기 -->
	<select id="selectCname" resultType="">
		SELECT DISTINCT CNAME FROM GC_THEATER
	</select>
	
	<!-- 지역 이름 가져오기 -->
	<select id="selectLname" resultType="" parameterType="">
		SELECT DISTINCT LNAME FROM GC_THEATER WHERE CNAME = #{cname}
	</select>
	
</mapper>















