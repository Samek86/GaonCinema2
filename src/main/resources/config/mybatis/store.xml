<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="store">

	<select id="select" resultType="com.gaon.cinema.store.StoreDTO">
	  select * from gc_store
	</select>

	<select id="detail" resultType="com.gaon.cinema.store.StoreDTO">
	  select * from gc_store where store_id=#{store_id}
	</select>
	
</mapper>